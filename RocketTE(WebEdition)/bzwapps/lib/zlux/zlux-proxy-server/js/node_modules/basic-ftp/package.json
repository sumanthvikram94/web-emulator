{
  "name": "basic-ftp",
  "version": "5.3.26",
  "description": "FTP client for Node.js, supports FTPS over TLS, IPv6, Async/Await, and Typescript.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run lint && tsc && mocha",
    "test": "npm run prepublishOnly",
    "clean": "rimraf dist",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint-fix": "eslint --fix \"./src/**/*.ts\"",
    "dev": "npm link basic-ftp && npm run clean && tsc --watch -p tsconfig.dev.json",
    "tdd": "mocha --watch",
    "buildOnly": "tsc"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.1",
    "@types/mocha": "7.0.2",
    "@types/node": "14.0.14",
    "@typescript-eslint/eslint-plugin": "3.5.0",
    "@typescript-eslint/parser": "3.5.0",
    "eslint": "6.8.0",
    "mocha": "7.2.0",
    "mock-fs": "4.12.0",
    "rimraf": "3.0.2",
    "typescript": "3.9.6"
  },
  "dependencies": {
    "archiver": "^5.0.2"
  }
}
